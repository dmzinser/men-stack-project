<div class="page-div"><%-include('../partials/header.ejs')%>
    <div class="header-div">
      <%if(!isLogged) {%>
        <%- include ../partials/head %>
          <%} else {%>
        <%-include('../partials/navlogged')%>
      <%}%>
    </div>
  <div class="body-div">
    <body class='account-body'>
      <div class='account-container'>
        <main>
            <h5>Create An Account</h5>
            <form action='/auth/register' method='POST'>
              <div class="row">
                  <div class="input-field col s6"><input id="textarea1" class="materialize-textarea" type='text' name='username'/>
                  <label for="textarea1">Username</label></div>
                  <div class='input-field col s6'><input id="textarea1" class="materialize-textarea" type='text' name='name'/>
                  <label for="textarea1">Name</label></div>
              </div>
              <div class="row">
                  <div class="input-field col s6"><input id="textarea1" class="materialize-textarea" type='text' name='location'/>
                  <label for="textarea1">Location</label></div>
                  <div class='input-field col s6'><input id="textarea1" class="materialize-textarea" type='email' name='email'/>
                  <label for="textarea1">Email</label></div>
              </div>
              <div class="row">
                  <div class="input-field col s6"><input id="textarea1" class="materialize-textarea" type='text' name='phone'/>
                  <label for="textarea1">Phone</label></div>
                  <div class='input-field col s6'><input id="textarea1" class="materialize-textarea" type='password' name='password'/>
                  <label for="textarea1">Password</label></div>
              </div>
              <div class="row">
                  <div class="col s12" id="login-account"><button class="waves-effect waves-light btn" type='submit'>Sign Up</div>
              </div>
            </form>
        </main>
      </div>
    </body>
  </div>
  <div class="footer-div">
  <%-include('../partials/footer.ejs')%>
  </div>
</div>